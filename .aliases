alias graph="git log --all --decorate --oneline --graph"
alias gl="git log --pretty=format:'%h > %<(20)%s' -10 | column -s '|'"
alias v="mvim -v"
alias compilescss="npm run scss"
alias ls="colorls -tr "
alias lc='colorls --sd'

alias myh="cd ~/.vim/plugged/vim-myhelp/doc"
